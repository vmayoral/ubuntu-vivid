&i2c2 {
       clock-frequency = <100000>;

       cape_eeprom0: cape_eeprom0@54 {
               compatible = "at,24c256";
               reg = <0x54>;
       };

       cape_eeprom1: cape_eeprom1@55 {
               compatible = "at,24c256";
               reg = <0x55>;
       };

       cape_eeprom2: cape_eeprom2@56 {
               compatible = "at,24c256";
               reg = <0x56>;
       };

       cape_eeprom3: cape_eeprom3@57 {
               compatible = "at,24c256";
               reg = <0x57>;
       };
};

